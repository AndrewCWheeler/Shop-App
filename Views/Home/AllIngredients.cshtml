@model AllIngredientsWrapper

<div class="col-sm-6 text-right">
                <a href="/dashboard">Dashboard</a> | <a href="/logout">Log Out</a>
            </div>



 <div class="row">
            <div class="col-sm-10 offset-sm-1">
                <table class="table">
                    <thead>
                        <th>Category Name</th>
                        <th>Discription</th>
                        <th>Action(s)</th>
                    </thead>
                    <tbody>
                        @foreach(Ingredient ing  in Model.AllIngredients)
                        {
                            <tr>
                                <td>@ing.IngredientName</td>
                                <td>@ing.Discription </td>
                                <td> placeholder   </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>