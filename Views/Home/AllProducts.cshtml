@model AllProductsWrapper

<div class="col-sm-6 text-right">
                <a href="/dashboard">Dashboard</a> | <a href="/logout">Log Out</a>
            </div>



 <div class="row">
            <div class="col-sm-10 offset-sm-1">
                <table class="table">
                    <thead>
                        <th>Product Name</th>
                        <th>Manufactured By</th>
                        <th>Discription</th>
                        <th>UPC</th>
                        <th>Action(s)</th>
                    </thead>
                    <tbody>
                        @foreach(Product pro  in Model.AllProducts)
                        {
                            <tr>
                                <td>@pro.ProductName</td>
                                <td>@pro.ManufacturedBy</td>
                                <td>@pro.Discription </td>
                                <td>@pro.UPC</td>
                                <td> placeholder   </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>